#include <iostream>

using namespace std;

int main()
{
    long age;
    cin>>age;
    int y=0;
    int m=0;

while(age>=365){
    age=age-365;
    y++;
 
}
  
while(age>=30){
    age=age-30;
    m++;
}
cout<<y<<" "<<"years"<<endl;
cout<<m<<" "<<"months"<<endl;
cout<<age<<" "<<"days"<<endl;
    
    


    return 0;
}
